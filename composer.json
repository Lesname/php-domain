{
  "name": "lesname/domain",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",

    "lesname/value-object": "^0.6.0",

    "doctrine/dbal": "^4.0.0",
    "ramsey/uuid": "^4.2.0",

    "psr/http-message": "^2.0.0",
    "psr/container": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "LesDomain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesDomainTest\\": "test/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^4.0.0",

    "phpunit/phpunit": "^12.0.0",

    "phpstan/phpstan": "^2.0.0",
    "phpstan/extension-installer": "^1.4.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
