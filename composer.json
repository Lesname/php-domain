{
  "name": "lessname/domain",
  "description": "Event source setup",
  "license": "proprietary",
  "repositories": [
    {
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json",
      "type": "composer"
    }
  ],
  "require": {
    "php": "^8.2.0",

    "lessname/value-object": "^0.3.0",

    "doctrine/dbal": "^3.1.0",
    "ramsey/uuid": "^4.2.0",

    "psr/http-message": "^1.0.0 || ^2.0.0",
    "psr/container": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^2.0.0",
    "phpunit/phpunit": "^10.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "LessDomain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessDomainTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
